{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "apk": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "local": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "EXPO_USE_LOCAL_CLI": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
